@model EntityLayer.Concrete.Job
@{
    ViewData["Title"] = "UpdateCustomer";
    Layout = "~/Views/Shared/UILayout.cshtml";
}

<h1>Meslek Güncelleme Sayfası</h1>

<form method="post">
    @Html.Label("Meslek Adı")
    @Html.TextBoxFor(x=>x.Name, new {@class="form-control"})
    @Html.ValidationMessageFor(x=>x.Name)
    <br />
    <button class="btn btn-outline-success">Kaydet</button>
</form>